<h1>Create Post</h1>
<h1>Post By UserId : {{userId}}</h1>
<form action="/api/post/{{userId}}" method="post">
    <label for="POST-title">Title:</label>
    <input type="text" id="POST-title" name="title">
    <label for="POST-content">Content:</label>
    <textarea type="text" id="POST-content" name="content"></textarea>
    {{!-- <label for="POST-userId">User Id:</label>
    <input type="text" id="POST-userId" name="user_id"> --}}
    <input type="submit" value="Create Post">
</form>